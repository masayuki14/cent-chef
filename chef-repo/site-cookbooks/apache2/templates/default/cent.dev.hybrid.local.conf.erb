# vi: syntax=apache ts=4 sw=4 sts=4 sr noet

<VirtualHost *:80>
   DocumentRoot "/home/vagrant/zf1/public"
   ServerName dev.hybrid.local

   # This should be omitted in the production environment
   SetEnv APPLICATION_ENV development

   <Directory "/home/vagrant/zf1/public">
       Options Indexes MultiViews FollowSymLinks
       AllowOverride All
       Order allow,deny
       Allow from all
   </Directory>

</VirtualHost>
